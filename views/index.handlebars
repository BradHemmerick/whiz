<body>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDomeXKvy8n2GG1lCgJ7KZLenY7atqHdNU&callback=initMap&libraries=places"></script>
    <div id="error"></div>
    <div id="error1"></div>

    <form id="busy">
        <h4 class="userType">Your Location: *</h4>
        <input type="text" name="address" id="input" placeholder="Ex: Address/City/State/Zip" class="form-control">
        <h4 class="userType">Name of Place</h4>
        <input type="text" min="0" name="location" class="form-control" id="location" placeholder="Ex: Name of store/bar/restaurant/etc">
        <div id="error" class="text-center"></div>
        <br>
        <input type="submit" value="Let's Go!" class="btn btn-primary" id="submit">
    </form>

    <div id="map">
    </div>

   
        <div id="place-1"></div><a id="button-1" data-id=""></a>
        <div id="place-2"></div><a id="button-2" data-id=""></a>
        <div id="place-3"></div><a id="button-3" data-id=""></a>
        <div id="place-4"></div><a id="button-4" data-id=""></a>
        <div id="place-5"></div><a id="button-5" data-id=""></a>




    <div id="error2">
    </div>
        

    <div id="list">
    {{{form}}}
    </div>

    <div id="comment">
    <p id="placeInfo"></p> 
    <ul id="list">
        {{#each comments}}
            <li>
                <p>User: {{this.user}}</p>
                <p>Comment: {{this.comment}}</p>
                <p>This comment has <inline class="upVote-{{this.id}}" data-upVote="{{this.upVotes}}">{{this.upVotes}}</inline> up votes and <inline class="downVote-{{this.id}}" data-downVote="{{this.downVotes}}">{{this.downVotes}}</inline> down votes</p>
                <p>Busy: <inline class="busy-{{this.id}}">{{this.busy}}</inline></p>
                <button data-id="{{this.id}}" class="deleteComment">Delete</button>
                <button data-id="{{this.id}}" data-voted="false" class="likeComment">Up Vote</button>
                <button data-id="{{this.id}}" data-voted="false" class="dislikeComment">Down Vote</button>
            </li>
        {{/each}}
    </ul>


    </div>
</body>